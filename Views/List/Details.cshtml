@model TESTT.Models.List

<h1>List Details</h1>

<div>
    <h4>List</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ListTitle)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ListTitle)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ListCategory)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ListCategory)
        </dd>
    </dl>views/Home/index
</div>

<h4>Tasks</h4>
<hr />
<ul>
    @foreach (var task in Model.Tasks)
    {
        <li>@task.TaskTitle</li>
    }
</ul>

<div>
    <a asp-action="Index">Back to List</a>
</div>
