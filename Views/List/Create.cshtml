@model TESTT.Models.List

<h1>Create</h1>

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

    <div class="form-group">
        <label asp-for="ListTitle" class="control-label"></label>
        <input asp-for="ListTitle" class="form-control" />
        <span asp-validation-for="ListTitle" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="ListCategory" class="control-label"></label>
        <input asp-for="ListCategory" class="form-control" />
        <span asp-validation-for="ListCategory" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>
